db_dft_important = db_dft[['MolecularVolume',
                           'SolvationEnergy',
                           'MolecularSurfaceArea',
                           'MolarMass',
                           'DipoleMoment',
                           'gap']]

db_rdkit_important = db_rdkit[['MolLogP',
